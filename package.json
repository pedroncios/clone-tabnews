{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "curso.dev study project",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "npm run docker:bd:up",
    "services:stop": "npm run docker:bd:stop",
    "services:down": "npm run docker:bd:down",
    "docker:bd:up": "docker compose -f infra/compose.yaml up -d",
    "docker:bd:stop": "docker compose -f infra/compose.yaml stop",
    "docker:bd:down": "docker compose -f infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migration:create": "node-pg-migrate --migrations-dir infra/migrations create",
    "migration:up": "node-pg-migrate --migrations-dir infra/migrations --envPath .env.development up"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.4",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.2.5"
  }
}
